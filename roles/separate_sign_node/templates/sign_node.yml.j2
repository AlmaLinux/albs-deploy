---
pgp_keys: {{ gpg_keys }}
jwt_token: "{{ albs_jwt_token }}"
pulp_host: "http://{{ albs_address }}:8081"
pulp_user: "{{ pulp_user }}"
pulp_password: "{{ pulp_password }}"
development_mode: true
dev_pgp_key_password: "{{ gpg_default_password }}"
master_url: "http://{{ albs_address }}:8080/api/v1/"
ws_master_url: "ws://{{ albs_address }}:8080/api/v1/"
{% if cas_api_key is defined %}
cas_api_key: {{ cas_api_key }}
{% endif %}
{% if cas_signer_id is defined %}
cas_signer_id: {{ cas_signer_id }}
{% endif %}
